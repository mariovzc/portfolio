<% mobile = true %>
<% style = 'material' %>
<%- partial('_partial/blog_nav') %>
<div class="container">
  <div class="row">
    <% site.posts.each(function(post){ %>
      <% if (post.title){ %>
          <div class="col s12 m4 ">
            <div class="card">
              <div class="card-image">
                <img src="<%= post.image %>">
                <span class="card-title"><%= post.title %></span>
              </div>
              <div class="card-content">
                <p class="truncate"><%- post.resume%></p>              
              </div>
              <div class="card-action center">
                <a href="<%- url_for(post.path) %>">Mas....</a>
              </div>
            </div>
          </div>            
      <%}%>
    <% }) %>
  </div> 
</div>